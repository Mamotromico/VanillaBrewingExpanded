<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<PipeSystem.ProcessDef>
		<defName>VBE_SyrupToSoda</defName>
		<label>Turn syrup into soda</label>
		<description>Turn 25 syrup into 25 soda.</description>
		<spawnOnInteractionCell>true</spawnOnInteractionCell>
		<autoExtract>false</autoExtract>
		<useIngredients>true</useIngredients>
		<transfersIngredientList>true</transfersIngredientList>
		<priorityInBillList>0</priorityInBillList>
		<ingredients>
			<li>
				<thing>VBE_SodaSyrup</thing>
				<countNeeded>25</countNeeded>
			</li>
		</ingredients>
		<ticks>120000</ticks>
		<results>
			<li>
				<thing>VBE_Soda</thing>
				<count>25</count>
			</li>
		</results>
		<noPowerDestroysProgress>true</noPowerDestroysProgress>
		<noPowerDestroysMessage>VBE_SyrupFailurePower</noPowerDestroysMessage>
		<noPowerDestroysInitialWarning>VCE_SyrupFailurePowerWarning</noPowerDestroysInitialWarning>
		<rareTicksToDestroy>10</rareTicksToDestroy>
		<destroyIngredientsOnStart>true</destroyIngredientsOnStart>
	</PipeSystem.ProcessDef>
	<PipeSystem.ProcessDef>
		<defName>VBE_AmbrandyMustToAmbrandy</defName>
		<label>Ferment Ambrandy Must into Ambrandy</label>
		<description>Turns 1 Ambrandy Must into 3 Ambrandy.</description>
		<labelOverride>Turn 1 Ambrandy Must into 3 Ambrandy.</labelOverride>
		<priorityInBillList>0</priorityInBillList>
		<spawnOnInteractionCell>true</spawnOnInteractionCell>
		<ingredients>
			<li>
				<thing>VBE_AmbrandyMust</thing>
				<countNeeded>1</countNeeded>
			</li>
		</ingredients>
		<ticksQuality>
			<li>900000</li>
			<li>1200000</li>
			<li>1800000</li>
			<li>2700000</li>
			<li>3900000</li>
			<li>5400000</li>
			<li>7200000</li>
		</ticksQuality>
		<results>
			<li>
				<thing>VBE_Ambrandy</thing>
				<count>3</count>
			</li>
		</results>
		<stopAtQuality>true</stopAtQuality>
		<allowExtractAtCurrentQuality>true</allowExtractAtCurrentQuality>
		<autoExtract>false</autoExtract>
	</PipeSystem.ProcessDef>
